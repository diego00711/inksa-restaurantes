#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Usuário/Desktop/meu-site-inksa-git/inksa-restaurantes/node_modules/.pnpm/vite@6.3.5_@types+node@20.1_1689f58c0d3b0d9596af47f1f59edc5a/node_modules/vite/bin/node_modules:/mnt/c/Users/Usuário/Desktop/meu-site-inksa-git/inksa-restaurantes/node_modules/.pnpm/vite@6.3.5_@types+node@20.1_1689f58c0d3b0d9596af47f1f59edc5a/node_modules/vite/node_modules:/mnt/c/Users/Usuário/Desktop/meu-site-inksa-git/inksa-restaurantes/node_modules/.pnpm/vite@6.3.5_@types+node@20.1_1689f58c0d3b0d9596af47f1f59edc5a/node_modules:/mnt/c/Users/Usuário/Desktop/meu-site-inksa-git/inksa-restaurantes/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Usuário/Desktop/meu-site-inksa-git/inksa-restaurantes/node_modules/.pnpm/vite@6.3.5_@types+node@20.1_1689f58c0d3b0d9596af47f1f59edc5a/node_modules/vite/bin/node_modules:/mnt/c/Users/Usuário/Desktop/meu-site-inksa-git/inksa-restaurantes/node_modules/.pnpm/vite@6.3.5_@types+node@20.1_1689f58c0d3b0d9596af47f1f59edc5a/node_modules/vite/node_modules:/mnt/c/Users/Usuário/Desktop/meu-site-inksa-git/inksa-restaurantes/node_modules/.pnpm/vite@6.3.5_@types+node@20.1_1689f58c0d3b0d9596af47f1f59edc5a/node_modules:/mnt/c/Users/Usuário/Desktop/meu-site-inksa-git/inksa-restaurantes/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
